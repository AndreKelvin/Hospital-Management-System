<VBox prefHeight="183.0" prefWidth="589.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hospitalpatienttreatment.DeletePatientTreatmentController">
   <children>
      <VBox>
         <children>
            <HBox>
               <children>
                  <Pane prefHeight="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <ImageView fitHeight="33.0" fitWidth="104.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icon/delete-512%20(2018_09_26%2007_59_24%20UTC).png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <Separator prefHeight="3.0" prefWidth="496.0" style="-fx-background-color: black;" />
         </children>
      </VBox>
      <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="95.0" prefWidth="600.0" text="Are your sure you want to delete?" VBox.vgrow="ALWAYS">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <HBox prefHeight="33.0" prefWidth="600.0">
         <children>
            <Pane maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="437.0" HBox.hgrow="ALWAYS" />
